<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!---Message displayed when the device is Network PUK locked -->
    <add-resource type="string" name="personalisation_puklocked" />
    <string name="personalisation_puklocked">Network PUK Locked</string>
    <!-- Service option entries.  -->
    <string-array name="tty_mode_entries">
        <item>TTY Off</item>
        <item>TTY Full</item>
    </string-array>

    <!-- Do not translate. -->
    <string-array name="tty_mode_values" translatable="false">
        <!-- Do not translate. -->
        <item>0</item>
        <!-- Do not translate. -->
        <item>1</item>
        <!-- Do not translate. -->
    </string-array>

    <string name="personalisation_puklocked">Network Unlock request Failed, Device PUK Locked.</string>

    <!-- Mobile network settings screen, femtocell enabling name -->
    <add-resource type="string" name="femtocell_enabled" />
    <string name="femtocell_enabled">Search for Microcell</string>

    <add-resource type="string" name="femtocell_enabled_progress" />
    <string name="femtocell_enabled_progress">Searching\u2026</string>

    <add-resource type="string" name="femtocell_enabled_failed" />
    <string name="femtocell_enabled_failed">Error while searching</string>
</resources>
